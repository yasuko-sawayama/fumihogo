{
  "presets": [
    "@babel/react",
    ["@babel/env",
      {
        "targets": {
          "browsers": [
              ">0.25%",
            "not ie 11",
            "not op_mini all"
          ]
        },
        useBuiltIns: 'usage'
      }
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "app"
      }
    ]
  ]
}
